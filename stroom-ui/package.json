{
  "name": "stroom-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.1",
    "mousetrap": "^1.6.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.3.2",
    "react-datepicker": "^1.4.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-semantic-redux-form": "^1.2.6",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "sjcl": "^1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "./config.template.sh && BROWSER=none PORT=5001 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook-js": "start-storybook -p 9001 -c .storybook",
    "storybook": "npm-run-all -p storybook-js watch-css",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.4",
    "@storybook/addon-notes": "^3.4.5",
    "@storybook/addons": "^3.4.4",
    "@storybook/react": "^3.4.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-node": "^6.0.1",
    "expect": "^22.4.3",
    "fetch-mock": "^6.4.2",
    "jest": "^20.0.3",
    "jest-fetch-mock": "^1.6.3",
    "lorem-ipsum": "^1.0.4",
    "prettier-eslint": "^8.8.1",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "standard": "^11.0.1",
    "storybook-react-router": "^1.0.1"
  },
  "jest": {}
}
