<?xml version="1.1" encoding="UTF-8"?>
<dataSplitter
        xmlns="data-splitter:3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="data-splitter:3 file://data-splitter-v3.0.xsd"
        version="3.0">

    <split delimiter="\n">
        <group value="$1">
            <all>
                <data value="$0"/>
            </all>
        </group>
    </split>
</dataSplitter>
