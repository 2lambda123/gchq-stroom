<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:DockLayoutPanel unit="PX" styleName="panel">
    <g:north size="5">
      <g:DockLayoutPanel unit="PX" width="100%"
        height="100%">
        <g:west size="5">
          <g:SimplePanel styleName="topLeft" />
        </g:west>
        <g:center>
          <g:SimplePanel styleName="top" />
        </g:center>
        <g:east size="5">
          <g:SimplePanel styleName="topRight" />
        </g:east>
      </g:DockLayoutPanel>
    </g:north>
    <g:center>
      <g:FlowPanel ui:field="content" styleName="content" />
    </g:center>
    <g:south size="5">
      <g:DockLayoutPanel unit="PX" width="100%"
        height="100%">
        <g:west size="5">
          <g:SimplePanel styleName="bottomLeft" />
        </g:west>
        <g:center>
          <g:SimplePanel styleName="bottom" />
        </g:center>
        <g:east size="5">
          <g:SimplePanel styleName="bottomRight" />
        </g:east>
      </g:DockLayoutPanel>
    </g:south>
  </g:DockLayoutPanel>
</ui:UiBinder> 
