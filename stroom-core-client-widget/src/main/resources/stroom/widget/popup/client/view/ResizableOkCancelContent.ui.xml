<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DockLayoutPanel unit="PX" styleName="max">
		<g:center>
			<g:ResizeLayoutPanel ui:field="content"
				styleName="max" />
		</g:center>
		<g:south size="35">
			<g:FlowPanel styleName="resizableOkCancelContent-buttons">
				<g:Button ui:field="ok" text="OK" />
				<g:Button ui:field="cancel" styleName="gwt-Button resizableOkCancelContent-cancel"
					text="Cancel" />
			</g:FlowPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>
