<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"><ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="res"
		type="stroom.widget.popup.client.view.SimplePopup.Resources" />
  
  <g:SimplePanel styleName="{res.style.container}">
<g:SimplePanel styleName="{res.style.background}">
    <g:SimplePanel ui:field="content" width="100%" height="100%" styleName="{res.style.content}"/>
  </g:SimplePanel>
  </g:SimplePanel>
</ui:UiBinder> 
