<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="res"
		type="stroom.widget.popup.client.view.Dialog.Resources" />

	<g:FlowPanel styleName="{res.style.container}">
		<g:FlowPanel styleName="{res.style.background}">
			<g:SimplePanel ui:field="content" styleName="{res.style.content}" />
			<g:SimplePanel styleName="{res.style.titleBar}">
				<g:Label ui:field="titleText" styleName="{res.style.titleText}">Caption</g:Label>
			</g:SimplePanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 
