<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:p="urn:import:stroom.data.pager.client">

  <ui:style>
    .max {
    	width: 100%;
    	height: 100%;
    }
    
    .white {
    	background-color: white;
    }
  </ui:style>

  <g:SimplePanel styleName='{style.max} {style.white}'>
    <c:CellTable ui:field='cellTable' addStyleNames='{style.max}' pageSize='100' />
  </g:SimplePanel>
</ui:UiBinder>
