             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:v="urn:import:stroom.widget.valuespinner.client"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form">
        <form:FormGroup identity="processorLimitsRecordLimit" label="Record Limit">
            <v:ValueSpinner ui:field="recordLimit" width="200px"/>
        </form:FormGroup>
        <form:FormGroup identity="processorLimitsTimeLimit" label="Time Limit (mins)">
            <v:ValueSpinner ui:field="timeLimit" width="200px"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder>
