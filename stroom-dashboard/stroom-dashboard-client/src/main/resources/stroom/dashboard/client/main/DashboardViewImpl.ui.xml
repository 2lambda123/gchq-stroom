<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:q="urn:import:stroom.dashboard.client.query">
    <g:FlowPanel styleName="DashboardViewImpl max dock-container-vertical DashboardViewImpl">
        <g:FlowPanel styleName="DashboardViewImpl-top dock-min dock-container-horizontal">
            <g:FlowPanel ui:field="left" styleName="dock-min button-container"/>
            <g:TextBox ui:field="params" addStyleNames="dock-max allow-focus"/>
            <q:QueryButtons ui:field="queryButtons" addStyleNames="dock-min"/>
        </g:FlowPanel>
        <g:SimplePanel ui:field="content" styleName="dock-max DashboardViewImpl-content"/>
    </g:FlowPanel>
</ui:UiBinder>
