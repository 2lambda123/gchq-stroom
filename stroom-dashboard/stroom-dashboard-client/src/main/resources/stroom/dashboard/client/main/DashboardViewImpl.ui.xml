<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:FlowPanel styleName="max dock-container-vertical dashboardViewImpl">
        <g:FlowPanel styleName="dock-min dock-container-horizontal">
            <g:FlowPanel ui:field="left" styleName="dock-min button-container dashboardViewImpl-leftInner"/>
            <g:TextBox ui:field="params"  addStyleNames="dashboardViewImpl-params allow-focus"/>
            <g:FlowPanel ui:field="right"/>
        </g:FlowPanel>
        <g:SimplePanel ui:field="content" styleName="dock-max dashboardViewImpl-content"/>
    </g:FlowPanel>
</ui:UiBinder>
