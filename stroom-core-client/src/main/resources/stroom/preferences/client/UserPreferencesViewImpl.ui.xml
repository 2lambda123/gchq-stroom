<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:stroom.widget.button.client"
             xmlns:tab="urn:import:stroom.widget.tab.client.view">
    <g:FlowPanel styleName="max dock-container-vertical form">
        <g:FlowPanel styleName="dock-min linkTabPanelViewImpl">
            <tab:LinkTabBar ui:field="tabBar" styleName="linkTabPanelViewImpl-middle linkTabBar"/>
        </g:FlowPanel>
        <tab:LayerContainerImpl ui:field="layerContainer" styleName="dock-max"/>
        <g:FlowPanel styleName="preferencesViewImpl-buttons button-group">
            <b:Button ui:field="setAsDefault" addStyleNames="allow-focus" text="Set As Default"/>
            <b:Button ui:field="revertToDefault" addStyleNames="allow-focus" text="Revert To Default"/>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>
