<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:l="urn:import:stroom.item.client"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form">
        <form:FormGroup identity="fieldEditType" label="Type">
            <l:ItemListBox ui:field="type" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="fieldEditName" label="Name">
            <g:TextBox ui:field="name" addStyleNames="w-100"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder> 
