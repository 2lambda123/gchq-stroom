<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:MySplitLayoutPanel unit="PX" width="100%" height="100%" vSplits="0.5">
        <g:north size="200">
            <g:SimplePanel ui:field="table" styleName="{max stroom-content stroom-border-bottom"/>
        </g:north>
        <g:center>
            <g:SimplePanel ui:field="expression" styleName="max stroom-content stroom-border-top"/>
        </g:center>
    </g:MySplitLayoutPanel>
</ui:UiBinder> 
