<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:v="urn:import:stroom.widget.tickbox.client.view"
             xmlns:i="urn:import:stroom.item.client"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form">
        <form:FormGroup identity="basicTextSettingsId" label="Id">
            <g:Label ui:field="id" addStyleNames="w-100 stroom-control-label"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsName" label="Name">
            <g:TextBox ui:field="name" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsTable" label="Table">
            <i:SelectionBox ui:field="table" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsStreamIdField" label="Stream Id Field">
            <i:SelectionBox ui:field="streamIdField" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsPartNoField" label="Part No Field">
            <i:SelectionBox ui:field="partNoField" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsRecordNoField" label="Record No Field">
            <i:SelectionBox ui:field="recordNoField" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsLineFromField" label="Line From Field">
            <i:SelectionBox ui:field="lineFromField" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsColFromField" label="Col From Field">
            <i:SelectionBox ui:field="colFromField" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsLineToField" label="Line To Field">
            <i:SelectionBox ui:field="lineToField" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsColToField" label="Col To Field">
            <i:SelectionBox ui:field="colToField" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="basicTextSettingsPipeline" label="Pipeline">
            <g:SimplePanel ui:field="pipeline" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup>
            <v:CustomCheckBox ui:field="showAsHtml" identity="basicTextSettingsShowAsHTML" label="Show As HTML"/>
        </form:FormGroup>
        <form:FormGroup>
            <v:CustomCheckBox ui:field="showStepping" identity="basicTextSettingsShowStepping" label="Show Stepping"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder> 
