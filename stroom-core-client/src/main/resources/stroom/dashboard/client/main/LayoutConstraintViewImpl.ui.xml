<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:tickbox="urn:import:stroom.widget.tickbox.client.view"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form-page form">
        <form:FormGroup>
            <tickbox:CustomCheckBox ui:field="fitWidth" label="Fit Width" identity="layoutConstraintFitWidth"/>
        </form:FormGroup>
        <form:FormGroup>
            <tickbox:CustomCheckBox ui:field="fitHeight" label="Fit Height" identity="layoutConstraintFitHeight"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder>
