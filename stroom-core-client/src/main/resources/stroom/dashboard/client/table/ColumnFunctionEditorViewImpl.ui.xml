<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
>
    <g:FlowPanel styleName="max default-min-sizes dock-container-vertical">
        <g:FlowPanel addStyleNames="dock-min dock-container-horizontal columnFunctionEditorViewImpl-labelPanel">
            <g:HTML styleName="columnFunctionEditorViewImpl-completionLabel">
                <code>ctrl-space</code>
                for auto-completion
            </g:HTML>
        </g:FlowPanel>
        <g:MySplitLayoutPanel styleName="max DashboardViewImpl">
            <g:north size="60">
                <g:SimplePanel ui:field="editorContainer" styleName="max columnFunctionEditorViewImpl-editorContainer"/>
            </g:north>
            <g:center>
                <g:SimplePanel ui:field="queryHelpContainer" styleName="max"/>
            </g:center>
        </g:MySplitLayoutPanel>
    </g:FlowPanel>
</ui:UiBinder>
