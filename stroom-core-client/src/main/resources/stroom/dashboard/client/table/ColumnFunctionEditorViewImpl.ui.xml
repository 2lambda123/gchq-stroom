<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:ThinSplitLayoutPanel styleName="max default-min-sizes columnFunctionEditorViewImpl">
        <g:west size="300">
            <g:SimplePanel ui:field="queryHelpContainer" styleName="max"/>
        </g:west>
        <g:center>
            <g:FlowPanel styleName="max dock-container-vertical columnFunctionEditorViewImpl-editorContainer">
                <g:SimplePanel ui:field="editorContainer" styleName="dock-max"/>
                <g:FlowPanel addStyleNames="dock-min dock-container-horizontal columnFunctionEditorViewImpl-labelPanel">
                    <g:HTML styleName="columnFunctionEditorViewImpl-completionLabel">
                        <code>ctrl-space</code>
                        for auto-completion
                    </g:HTML>
                </g:FlowPanel>
            </g:FlowPanel>
        </g:center>
    </g:ThinSplitLayoutPanel>
</ui:UiBinder>
