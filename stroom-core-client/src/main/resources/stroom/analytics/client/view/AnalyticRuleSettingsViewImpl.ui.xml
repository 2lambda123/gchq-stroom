<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:item="urn:import:stroom.item.client"
             xmlns:l="urn:import:stroom.item.client"
             xmlns:d="urn:import:stroom.widget.customdatebox.client"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form-padding form">
        <form:FormGroup identity="analyticRuleSettingsLanguageVersion" label="Language Version">
            <item:SelectionBox ui:field="languageVersion" width="400px"/>
        </form:FormGroup>
        <form:FormGroup identity="analyticRuleSettingsAlertRuleType" label="Alert Rule Type">
            <l:SelectionBox ui:field="analyticRuleType"/>
        </form:FormGroup>
        <form:FormGroup identity="analyticRuleSettingsDataRetention" label="Time To Keep Data In The Store">
            <d:DurationPicker ui:field="dataRetention"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder> 
