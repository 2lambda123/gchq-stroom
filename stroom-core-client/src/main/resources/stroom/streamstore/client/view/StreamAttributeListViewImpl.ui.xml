<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:stroom.widget.button.client"
  xmlns:tb="urn:import:stroom.widget.tickbox.client.view">
  <ui:style>
    .outer {
    	width: 400px;
    	height: 60px;
    	border: 1px solid #C5CDE2;
    	background-color: white;
    }
  </ui:style>
  <g:DockLayoutPanel unit="PX" styleName="{style.outer}">
    <g:south size="18">
      <g:FlowPanel>
        <v:GlyphButton ui:field="add" />
        <v:GlyphButton ui:field="remove" />
      </g:FlowPanel>
    </g:south>
    <g:center>
      <g:ScrollPanel ui:field="list" width="100%" height="100%" />
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder> 
