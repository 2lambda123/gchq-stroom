             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:FlowPanel ui:field="layout" styleName="max default-min-sizes dock-container-vertical">
        <g:FlowPanel styleName="dock-min input-row">
            <g:Label text="Volume Group Name:"/>
            <g:TextBox ui:field="name" addStyleNames="dock-max"/>
