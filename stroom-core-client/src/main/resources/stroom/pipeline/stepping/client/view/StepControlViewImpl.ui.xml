<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:i="urn:import:stroom.widget.button.client">
    <ui:style>
        .layout {
        display: inline-block;
        }
    </ui:style>
    <g:FlowPanel styleName="{style.layout}">
        <i:GlyphButton ui:field="refreshButton"/>
        <i:GlyphButton ui:field="lastButton"/>
        <i:GlyphButton ui:field="forwardButton"/>
        <i:GlyphButton ui:field="backwardButton"/>
        <i:GlyphButton ui:field="firstButton"/>
    </g:FlowPanel>
</ui:UiBinder>
