<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client"
             xmlns:c="urn:import:stroom.widget.customdatebox.client">
    <g:FlowPanel styleName="max form">
        <form:FormGroup identity="quickSettings" label="Quick Settings" addStyleNames="dock-min">
            <g:SimplePanel ui:field="quickSettings" addStyleNames="w-100"/>
        </form:FormGroup>
        <form:FormGroup identity="timeRange" label="After Time (inclusive)" addStyleNames="dock-min">
            <c:MyDateBox ui:field="timeFrom"/>
        </form:FormGroup>
        <form:FormGroup identity="timeRange" label="Before Time (exclusive)" addStyleNames="dock-min">
            <c:MyDateBox ui:field="timeTo"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder>
