<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:l="urn:import:stroom.item.client"
             xmlns:c="urn:import:stroom.widget.customdatebox.client"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FormPanel ui:field="form">
        <g:FlowPanel styleName="stroom-control-page">
            <form:FormRow identity="dataUploadMetaData" label="Meta Data">
                <g:TextArea ui:field="metaData" addStyleNames="w-100"/>
            </form:FormRow>
            <form:FormRow identity="dataUploadType" label="Type">
                <l:StringListBox ui:field="type" addStyleNames="w-100"/>
            </form:FormRow>
            <form:FormRow identity="dataUploadEffectiveDate" label="Effective Date">
                <c:MyDateBox ui:field="effective" addStyleNames="w-100"/>
            </form:FormRow>
            <form:FormRow identity="dataUploadFile" label="File">
                <g:FileUpload ui:field="fileUpload" addStyleNames="itemCheckList w-100" name="fileUpload"/>
            </form:FormRow>
        </g:FlowPanel>
    </g:FormPanel>
</ui:UiBinder>
