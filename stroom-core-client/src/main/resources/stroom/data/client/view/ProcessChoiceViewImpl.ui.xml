<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:tickbox="urn:import:stroom.widget.tickbox.client.view"
             xmlns:v="urn:import:stroom.widget.valuespinner.client"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form-page form">
        <form:FormGroup identity="processChoicePriority" label="Priority of new filters (unless auto)">
            <v:ValueSpinner ui:field="priority"/>
        </form:FormGroup>
        <form:FormGroup identity="processChoiceAuto" label="Attempt to automatically set priority">
            <tickbox:CustomCheckBox ui:field="autoPriority"/>
        </form:FormGroup>
        <form:FormGroup identity="processChoiceReprocess" label="Reprocess data">
            <tickbox:CustomCheckBox ui:field="reprocess"/>
        </form:FormGroup>
        <form:FormGroup identity="processChoiceEnable" label="Enable new filters">
            <tickbox:CustomCheckBox ui:field="enabled"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder>
